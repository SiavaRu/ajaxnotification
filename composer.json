{
	"name": "tas2580/ajaxnotification",
	"type": "phpbb-extension",
	"description": "Queries notifications per AJAX, so the user get notifications without refreshing the page.",
	"homepage": "https://tas2580.net/downloads/phpbb-ajaxnotification/",
	"version": "0.1.0",
	"keywords": ["phpbb", "extension", "wiki"],
	"license": "GPL-2.0",
	"authors": [{
			"name": "tas2580",
			"email": "mail@tas2580.net",
			"homepage": "https://tas2580.net",
			"role": "Developer"
		}],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "AJAX Notification",
		"soft-require": {
			"phpbb/phpbb": ">=3.1.0-RC2,<3.2.0@dev"
		},
		"version-check": {
			"host": "phpbb-versionscheck.cf",
			"directory": "/tas2580/ajaxnotification/master",
			"filename": "version-check.json"
		}
	}
}